            <xml-property name="queryText"><![CDATA[select p.full_name, 'FIDUCIARIO' type_quoter, ir.rate/100:: decimal rate,ir.tax/100:: decimal tax,
case 
	when q.rate_type ='MONTHLY' then 'MENSUAL NIVELADO'
	else 'QUINCENAL NIVELADO'
end rate_type,q.loan_amount,q.disbursement_date ,q.first_installment_date,q.term_in_months,c.code currency 
	from main.quoter q 
		join main.person p on q.customer_id =p.id
		join main.interest_rate ir on q.interest_rate_id = ir.id 
		join main.currency c on q.currency_id=c.id
		where q.active =true and q.id=?:: INTEGER]]></xml-property>
